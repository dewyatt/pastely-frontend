{
  "name": "pastely-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/* && cp -a static/ dist/",
    "start": "NODE_ENV=development webpack-dev-server --config webpack.config.local.js --host 0.0.0.0 --progress --color",
    "build:webpack-staging": "NODE_ENV=production webpack -p --config webpack.config.staging.js --progress --color",
    "build:webpack-production": "NODE_ENV=production webpack -p --config webpack.config.production.js --progress --color",
    "build:staging": "npm run clean && npm run build:webpack-staging",
    "build:production": "npm run clean && npm run build:webpack-production",
    "test:lint": "eslint ./src --ext .js --cache"
  },
  "author": "Daniel Wyatt <daniel@dewyatt.com> (http://www.dewyatt.com)",
  "license": "ISC",
  "dependencies": {
    "brace": "^0.8.0",
    "react": "^15.1.0",
    "react-ace": "^3.4.1",
    "react-dom": "^15.1.0",
    "react-modal": "^1.3.0",
    "react-router": "^2.4.1",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.19.0",
    "json-loader": "^0.5.4",
    "muicss": "^0.5.9",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-validator": "^2.1.2"
  }
}
